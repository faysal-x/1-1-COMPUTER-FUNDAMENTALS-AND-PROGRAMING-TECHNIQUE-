//Write a C program to find maximum between three numbers

#include<stdio.h>

int main(){

int a, b, c;

    printf("Enter a: ");
    scanf("%d", &a);

    printf("Enter b: ");
    scanf("%d", &b);

    printf("Enter c: ");
    scanf("%d", &c);

    if(a>b && a>c){
    printf("a is bigger.");
    }else if(b>a && b > c){
    printf("b is bigger.");
    }else{
    printf("c is bigger.");
    }

return 0;
}
